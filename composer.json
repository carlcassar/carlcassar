{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": [
        "laravel",
        "framework"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.3",
        "aws/aws-sdk-php": "^3.279",
        "cassarco/markdown-tools": "^2.0",
        "filament/filament": "^5.0",
        "guzzlehttp/guzzle": "^7.2",
        "laravel/breeze": "^2.0",
        "laravel/framework": "^v12.0.0",
        "laravel/horizon": "^5.19",
        "laravel/sanctum": "^4",
        "laravel/tinker": "^2.10.1",
        "livewire/livewire": "^v4.0.3",
        "ryangjchandler/laravel-cloudflare-turnstile": "^v3.0.1",
        "spatie/laravel-feed": "^4.3",
        "spatie/laravel-sitemap": "^7.2.0",
        "symfony/yaml": "^6.3"
    },
    "require-dev": {
        "beyondcode/laravel-dump-server": "^2",
        "fakerphp/faker": "^1.9.1",
        "filament/upgrade": "^5.0",
        "laravel/pint": "^1.24",
        "laravel/sail": "^1.41",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^8.6",
        "pestphp/pest": "v4.3.2",
        "pestphp/pest-plugin-laravel": "^v4.0.0",
        "spatie/laravel-ignition": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan filament:upgrade"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
